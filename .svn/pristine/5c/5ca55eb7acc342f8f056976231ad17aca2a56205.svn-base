package com.creditharmony.core.deduct.bean.out;

import java.util.Date;
import java.util.List;

/**
 * 划扣后返回数据
 * @Class Name LoanDeductEntity
 * @author 王彬彬
 * @Create In 2016年2月2日
 */
public class LoanDeductEntity{
	private String id;
	private String remarks;	// 备注
	private String createBy;	// 创建者
	private Date createTime;	// 创建时间
	private String modifyBy;	// 更新者
	private Date modifyTime;	// 更新时间
	
	/**
	 * 系统处理ID
	 * */
	private String sysId;

	/** 请求ID */
	private String requestId;
	
	/**
	 * 业务ID (合同编号)
	 * */
	private String businessId;

	/**
	 * 请求批次ID （拆分关联ID）
	 * */
	private String batId;

	/**
	 * 关联ID 期供ID
	 * */
	private String refId;

	/**
	 * 申请还款金额
	 */
	private String applyAmount;

	/**
	 * 成功金额
	 */
	private String deductSucceedMoney;

	/**
	 * 失败金额
	 */
	private String deductFailMoney;

	/**
	 * 交易时间
	 */
	private String deductTime;

	/**
	 * 划扣标识
	 */
	private String deductSysIdType;
	
	/**
	 * 失败原因
	 */
	private String failReason;
	
	/**
	 * 中金划扣次数
	 */
	private int deductNum;
	
	/**
	 * 通联划扣次数
	 */
	private int tlDeductNum;
	
	/**
	 * 划扣用卡号
	 */
	private String accountNo;

	/**
	 * 拆分
	 */
	private List<PaybackSplitEntityEx> splitData;

	public String getSysId() {
		return sysId;
	}

	public String getBatId() {
		return batId;
	}

	public String getRefId() {
		return refId;
	}

	public String getApplyAmount() {
		return applyAmount;
	}

	public String getDeductSucceedMoney() {
		return deductSucceedMoney;
	}

	public String getDeductFailMoney() {
		return deductFailMoney;
	}

	public String getDeductTime() {
		return deductTime;
	}

	public String getDeductSysIdType() {
		return deductSysIdType;
	}

	public List<PaybackSplitEntityEx> getSplitData() {
		return splitData;
	}

	public void setSysId(String sysId) {
		this.sysId = sysId;
	}

	public void setBatId(String batId) {
		this.batId = batId;
	}

	public void setRefId(String refId) {
		this.refId = refId;
	}

	public void setApplyAmount(String applyAmount) {
		this.applyAmount = applyAmount;
	}

	public void setDeductSucceedMoney(String deductSucceedMoney) {
		this.deductSucceedMoney = deductSucceedMoney;
	}

	public void setDeductFailMoney(String deductFailMoney) {
		this.deductFailMoney = deductFailMoney;
	}

	public void setDeductTime(String deductTime) {
		this.deductTime = deductTime;
	}

	public void setDeductSysIdType(String deductSysIdType) {
		this.deductSysIdType = deductSysIdType;
	}

	public void setSplitData(List<PaybackSplitEntityEx> splitData) {
		this.splitData = splitData;
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getRemarks() {
		return remarks;
	}

	public void setRemarks(String remarks) {
		this.remarks = remarks;
	}

	public String getCreateBy() {
		return createBy;
	}

	public void setCreateBy(String createBy) {
		this.createBy = createBy;
	}

	public Date getCreateTime() {
		return createTime;
	}

	public void setCreateTime(Date createTime) {
		this.createTime = createTime;
	}

	public String getModifyBy() {
		return modifyBy;
	}

	public void setModifyBy(String modifyBy) {
		this.modifyBy = modifyBy;
	}

	public Date getModifyTime() {
		return modifyTime;
	}

	public void setModifyTime(Date modifyTime) {
		this.modifyTime = modifyTime;
	}

	public String getBusinessId() {
		return businessId;
	}

	public void setBusinessId(String businessId) {
		this.businessId = businessId;
	}

	/**
	 * @return the requestId
	 */
	public String getRequestId() {
		return requestId;
	}

	/**
	 * @param requestId the requestId to set
	 */
	public void setRequestId(String requestId) {
		this.requestId = requestId;
	}

	public String getFailReason() {
		return failReason;
	}

	public void setFailReason(String failReason) {
		this.failReason = failReason;
	}

	public String getAccountNo() {
		return accountNo;
	}

	public void setAccountNo(String accountNo) {
		this.accountNo = accountNo;
	}

	public int getDeductNum() {
		return deductNum;
	}

	public void setDeductNum(int deductNum) {
		this.deductNum = deductNum;
	}

	public int getTlDeductNum() {
		return tlDeductNum;
	}

	public void setTlDeductNum(int tlDeductNum) {
		this.tlDeductNum = tlDeductNum;
	}

}
